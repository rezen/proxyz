{
	"name": "rezen/proxyz",
	"type": "library",
	"description": "A library to make it easier to change native php functions behaviour",
	"keywords": [
		"testing",
		"proxy"
	],
	"homepage": "https://github.com/rezen/proxyz",
	"support": {
		"issues": "https://github.com/rezen/proxyz/issues",
		"source": "https://github.com/rezen/proxyz"
	},
	"license": "MIT",
	"authors": [{
		"name": "Andres Hermosilla",
		"homepage": "https://ahermosilla.com"
	}],
	"prefer-stable": true,
	"minimum-stability": "dev",
	"bin": [],
	"require": {
		"php": ">=7.2.5"
	},
	"require-dev": {
		"symfony/dom-crawler": "^5.1",
		"symfony/css-selector": "^5.1",
		"phpunit/phpunit": "^8.0"
	},
	"autoload": {
		"psr-4": {},
		"files": [
			"src/Proxyz/wirer.php",
			"src/Proxyz/Delegate.php",
			"src/Proxyz/Wp/Misc.php",
			"src/Proxyz/Wp/Comment.php",
			"src/Proxyz/Wp/Http.php",
			"src/Proxyz/Wp/User.php",
			"src/Proxyz/Wp/Theme.php",
			"src/Proxyz/Wp/Plugin.php",
			"src/Proxyz/Wp/Post.php",
			"src/Proxyz/Wp/Formatting.php",
			"src/Proxyz/Wp/Multisite.php",
			"src/Proxyz/Wp/Feed.php",
			"src/Proxyz/Wp/Taxonomy.php",
			"src/Proxyz/Php/Network.php",
			"src/Proxyz/Php/Ftp.php",
			"src/Proxyz/Php/Mysqli.php",
			"src/Proxyz/Php/Socket.php",
			"src/Proxyz/Php/Openssl.php",
			"src/Proxyz/Php/System.php",
			"src/Proxyz/Php/Curl.php",
			"src/Proxyz/Php/Xml.php",
			"src/Proxyz/Php/Session.php",
			"src/Proxyz/Php/Filesystem.php",
			"src/Proxyz/Php/Mail.php",
			"src/Proxyz/Php/Info.php",
			"src/Proxyz/Php/Stream.php",
			"src/Proxyz/Php/Datetime.php"
		],
		"exclude-from-classmap": []
	},
	"autoload-dev": {
		"psr-4": {
			"Proxyz\\Tests\\": "test/tests",
			"Sample\\":"test/sample"
		}
	}
}